{
    "python.defaultInterpreterPath": "${workspaceFolder}/.env3.11/bin/python3",
    "python.formatting.blackPath": "${workspaceFolder}/.env3.11/bin/black",
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "editor.formatOnType": false,
    "editor.formatOnSave": true,
    "diffEditor.codeLens": false,
    "editor.rulers": [
        90
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true
    },
    "python.envFile": "${workspaceFolder}/.env3.11",
    "python.analysis.typeCheckingMode": "off",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "cSpell.words": [
        "adrybakov",
        "Andrey",
        "Angstrom",
        "aniso",
        "asymm",
        "asymm_matrix",
        "bmatrix",
        "boldsymbol",
        "Bravais",
        "currentmodule",
        "np.diag",
        "doctest",
        "Dzyaloshinskii",
        "iscomplex",
        "Kpoints",
        "lepage",
        "linalg",
        "magmom",
        "mathbf",
        "matplotlib",
        "Moriya",
        "ndarray",
        "niggli",
        "numpy",
        "phonopy",
        "projwfc",
        "pytest",
        "radtools",
        "Rybakov",
        "scipy",
        "spinham",
        "symm",
        "symm_matrix",
        "termcolor",
        "TODEGREES",
        "TORADIANS",
        "tqdm",
        "voronoi"
    ],
    "search.useIgnoreFiles": true,
}